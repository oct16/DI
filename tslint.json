{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {
        "object-literal-sort-keys": false
    },
    "rules": {
        "object-literal-sort-keys": false,
        "interface-name": false,
        "no-conditional-assignment": false,
        "no-namespace": [
            true,
            "allow-declarations"
        ],
        "ban-types": false,
        "variable-name": false,
        "no-console": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}
